{
  "expo": {
    "name": "meny-food-scanner",
    "slug": "meny-food-scanner",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/scannerIcon.png",
    "splash": {
      "image": "./app/assets/scannerIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/scannerIcon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/scannerIcon.png"
    },
    "description": "Scan food items from Meny and see the nutritional content and alternative options",
    "githubUrl": "https://github.com/seriousCookies/foodFinder"
  }
}
